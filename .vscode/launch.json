  "version": "0.2.0",
  "configurations": [
    {
      "name": "Mossy Vibes - Test Mode",
      "request": "launch",
      "type": "dart",
      "args": ["-d", "chrome"],
      "preLaunchTask": "Mossy Vibes: Prebuild"
    },
    {
      "name": "Mossy Vibes - Dev Mode (iOS)",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define-from-file",
        ".env.json",
        "-d",
        "B28DFB03-4ABC-4757-BF95-B16B79AC813"
      ],
      "preLaunchTask": "Mossy Vibes: Prebuild"
    },
    {
      "name": "Mossy Vibes - Dev Mode (web)",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define-from-file", ".env.json", "-d", "chrome"],
      "preLaunchTask": "Mossy Vibes: Prebuild"
    },
    {
      "name": "Mossy Vibes - Profile Mode",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "Mossy Vibes - Release Mode",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
